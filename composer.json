{
  "name": "mocean/laravel-notification-channel",
  "description": "Mocean Notification Channel for laravel.",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "illuminate/notifications": "~5 || ^6.0",
    "mocean/laravel-mocean": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5 || ~8.0 || ~9.0",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Mocean\\Notification\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mocean\\Notification\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mocean\\Notification\\MoceanChannelServiceProvider"
      ]
    }
  }
}
